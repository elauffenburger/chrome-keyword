{
  "name": "Chrome Keyword Search",
  "description": "",
  "omnibox": {
    "keyword": "kw"
  },
  "icons": {

  },
  "background": {
    "scripts": ["scripts/background.js", "scripts/jquery.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/xpath.js", "scripts/formFields.js"]
    }
  ],
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "contextMenus",
    "<all_urls>"
  ],
  "version": "1.0",
  "minimum_chrome_version": "22",
  "manifest_version": 2
}
